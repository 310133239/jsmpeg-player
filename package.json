  },
  "homepage": "https://github.com/cycjimmy/jsmpeg-player#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.0",
    "clean-webpack-plugin": "^0.1.16",
    "cross-env": "^5.0.5",
    "gulp": "^3.9.1",
    "gulp-copy": "^1.0.1",
    "gulp-gh-pages": "^0.5.4",
    "jest": "^21.1.0",
