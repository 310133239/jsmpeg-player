{
  "name": "jsmpeg-player",
  "version": "1.0.1",
  "description": "MPEG1 Video Player Based On JSMpeg",
  "main": "dist/JSMpeg.js",
  "module": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
